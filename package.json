{
  "name": "pkbmpemudapelopor",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --webpack",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "migrate:dry-run": "cross-env NODE_OPTIONS=--no-deprecation tsx --env-file=.env scripts/migrateToCloudinary.ts --dry-run",
    "migrate:run": "cross-env NODE_OPTIONS=--no-deprecation tsx --env-file=.env scripts/migrateToCloudinary.ts",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@payloadcms/admin-bar": "3.76.1",
    "@payloadcms/db-postgres": "3.76.1",
    "@payloadcms/live-preview-react": "3.76.1",
    "@payloadcms/next": "3.76.1",
    "@payloadcms/payload-cloud": "3.76.1",
    "@payloadcms/plugin-cloud-storage": "3.76.1",
    "@payloadcms/plugin-form-builder": "3.76.1",
    "@payloadcms/plugin-nested-docs": "3.76.1",
    "@payloadcms/plugin-redirects": "3.76.1",
    "@payloadcms/plugin-search": "3.76.1",
    "@payloadcms/plugin-seo": "3.76.1",
    "@payloadcms/richtext-lexical": "3.76.1",
    "@payloadcms/translations": "3.76.1",
    "@payloadcms/ui": "3.76.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.9.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "framer-motion": "^12.29.2",
    "geist": "^1.5.1",
    "graphql": "^16.12.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.564.0",
    "next": "16.2.0-canary.46",
    "next-sitemap": "^4.2.3",
    "payload": "3.76.1",
    "prism-react-renderer": "^2.4.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "7.71.1",
    "react-icons": "^5.5.0",
    "sharp": "0.34.2",
    "swiper": "^12.1.0",
    "tailwind-merge": "^2.6.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "1.54.1",
    "@supabase/supabase-js": "^2.93.3",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.4",
    "@types/formidable": "^3.4.6",
    "@types/leaflet": "^1.9.21",
    "@types/node": "22.5.4",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.24",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.2.0-canary.46",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.19",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "20.9.0",
    "pnpm": "^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ],
    "overrides": {
      "@types/react": "19.2.14",
      "@types/react-dom": "19.2.3"
    }
  }
}
